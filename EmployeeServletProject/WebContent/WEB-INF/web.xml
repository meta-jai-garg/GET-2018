<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://xmlns.jcp.org/xml/ns/javaee"
	xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
	id="WebApp_ID" version="3.1">
	<display-name>EmployeeServletProject</display-name>
	<servlet>
		<servlet-name>AddEmployee</servlet-name>
		<servlet-class>com.servlet.AddEmployee</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>AddEmployee</servlet-name>
		<url-pattern>/AddEmployee</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>ShowEmployees</servlet-name>
		<servlet-class>com.servlet.ShowEmployees</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>ShowEmployees</servlet-name>
		<url-pattern>/ShowEmployee</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>SearchEmployee</servlet-name>
		<servlet-class>com.servlet.SearchEmployee</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>SearchEmployee</servlet-name>
		<url-pattern>/SearchEmployee</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>EditEmployee</servlet-name>
		<servlet-class>com.servlet.EditEmployee</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>EditEmployee</servlet-name>
		<url-pattern>/EditEmployee</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>UpdateEmployee</servlet-name>
		<servlet-class>com.servlet.UpdateEmployee</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>UpdateEmployee</servlet-name>
		<url-pattern>/UpdateEmployee</url-pattern>
	</servlet-mapping>
	<welcome-file-list>
		<welcome-file>index.html</welcome-file>
	</welcome-file-list>
</web-app>